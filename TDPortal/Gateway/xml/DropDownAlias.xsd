<?xml version="1.0"?>
<xs:schema id="dropdownalias" 
			targetNamespace="http://www.transportdirect.info/DropDownAlias" 
			xmlns:mstns="http://www.transportdirect.info/DropDownAlias" 
			xmlns="http://www.transportdirect.info/DropDownAlias" 
			xmlns:xs="http://www.w3.org/2001/XMLSchema" 
			xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" 
			attributeFormDefault="qualified" 
			elementFormDefault="qualified">
    <xs:element name="dropdownalias" msdata:IsDataSet="true" msdata:Locale="en-GB" msdata:EnforceConstraints="False">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="Alias">
                    <xs:complexType>
                        <xs:sequence>
							<xs:element name="StopType" type="xs:string" minOccurs="0"/>
                            <xs:element name="StopNaPTAN" type="xs:string" minOccurs="0"/>
                            <xs:element name="AliasName" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:complexType>
    </xs:element>
</xs:schema>