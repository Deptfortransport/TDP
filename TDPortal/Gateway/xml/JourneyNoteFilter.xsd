<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="JourneyNoteFilter" 
		targetNamespace="http://www.transportdirect.info/JourneyNoteFilter" 
		xmlns:mstns="http://www.transportdirect.info/JourneyNoteFilter" 
		xmlns="http://www.transportdirect.info/JourneyNoteFilter" 
		xmlns:xs="http://www.w3.org/2001/XMLSchema" 
		xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" 
		attributeFormDefault="qualified" 
		elementFormDefault="qualified">
	<xs:element name="JourneyNoteFilterData" msdata:IsDataSet="true" msdata:Locale="en-GB" msdata:EnforceConstraints="False">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="JourneyNoteFilter">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Mode" type="xs:string" minOccurs="1" />
							<xs:element name="Region" type="xs:string" minOccurs="1" />
							<xs:element name="AccessibleOnly" type="xs:string" minOccurs="1" />
							<xs:element name="FilterText" type="xs:string" minOccurs="1" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>
