<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="airportlinks"
                  targetNamespace="http://www.transportdirect.info/AirportLinks"
                  xmlns="http://www.transportdirect.info/AirportLinks"
                  xmlns:mstns="http://www.transportdirect.info/AirportLinks"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  xmlns:msdata="urn:schemas-microsoft-com:xml-msdata"
				  elementFormDefault="qualified"
                  attributeFormDefault="qualified">
	<xs:element name="airportlinks" msdata:IsDataSet="true" msdata:Locale="en-GB" msdata:EnforceConstraints="False">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="AirportLink">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="NaPTAN" type="xs:string" />
							<xs:element name="DepartureBoard" type="xs:string" minOccurs="0" />
							<xs:element name="ArrivalsBoard" type="xs:string" minOccurs="0" />
							<xs:element name="GeneralInformation" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>