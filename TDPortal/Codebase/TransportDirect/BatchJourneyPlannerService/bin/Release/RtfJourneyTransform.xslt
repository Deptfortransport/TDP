  <xsl:template name="outputDuration">
    <xsl:param name="duration" />

    <xsl:if test="contains($duration, ':')">
      <xsl:variable name="hours">
        <xsl:call-template name="stripLeadingZeroes">
          <xsl:with-param name="value" select="substring-before($duration, ':')" />
        </xsl:call-template>
      </xsl:variable>

      <xsl:variable name="minutes">
        <xsl:call-template name="stripLeadingZeroes">
          <xsl:with-param name="value" select="substring-after($duration, ':')" />
        </xsl:call-template>
      </xsl:variable>

      <xsl:choose>
        <xsl:when test="string-length($hours) &gt; 0">
          <xsl:value-of select="$hours"/>
          <xsl:choose>
            <xsl:when test="$hours = '1'">
              <xsl:choose>
                <xsl:when test="string-length($minutes) = 0">
                  <xsl:text> hour</xsl:text>
                </xsl:when>
                <xsl:otherwise>
                  <xsl:text> hour and </xsl:text>
                  <xsl:value-of select="$minutes"/>
                  <xsl:choose>
                    <xsl:when test="$minutes = '1'">
                      <xsl:text> minute</xsl:text>
                    </xsl:when>
                    <xsl:otherwise>
                      <xsl:text> minutes</xsl:text>
                    </xsl:otherwise>
                  </xsl:choose>
                </xsl:otherwise>
              </xsl:choose>
            </xsl:when>
            <xsl:otherwise>
              <xsl:choose>
                <xsl:when test="string-length($minutes) = 0">
                  <xsl:text> hours</xsl:text>
                </xsl:when>
                <xsl:otherwise>
                  <xsl:text> hours and </xsl:text>
                  <xsl:value-of select="$minutes"/>
                  <xsl:choose>
                    <xsl:when test="$minutes = '1'">
                      <xsl:text> minute</xsl:text>
                    </xsl:when>
                    <xsl:otherwise>
                      <xsl:text> minutes</xsl:text>
                    </xsl:otherwise>
                  </xsl:choose>
                </xsl:otherwise>
              </xsl:choose>
            </xsl:otherwise>
          </xsl:choose>
        </xsl:when>
        <xsl:otherwise>
          <xsl:choose>
            <xsl:when test="string-length($minutes) &gt; 0">
              <xsl:value-of select="$minutes"/>
              <xsl:choose>
                <xsl:when test="$minutes = '1'">
                  <xsl:text> minute</xsl:text>
                </xsl:when>
                <xsl:otherwise>
                  <xsl:text> minutes</xsl:text>
                </xsl:otherwise>
              </xsl:choose>
            </xsl:when>
          </xsl:choose>
        </xsl:otherwise>
      </xsl:choose>
    </xsl:if>
  </xsl:template>

  <xsl:template name="stripLeadingZeroes">
    <xsl:param name="value"/>
    <xsl:choose>
      <xsl:when test="starts-with($value, '0')">
        <xsl:call-template name="stripLeadingZeroes">
          <xsl:with-param name="value" select="substring-after($value, '0')" />
        </xsl:call-template>
      </xsl:when>
      <xsl:otherwise>
        <xsl:value-of select="$value"/>
      </xsl:otherwise>
    </xsl:choose>
  </xsl:template>
</xsl:stylesheet>
