<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appSettings>
		<!-- property provider info -->
		<add key="propertyservice.applicationid" value="TTBOHost"/>
		<add key="propertyservice.groupid" value="TTBO"/>
		<add key="propertyservice.providerclass" value="TransportDirect.Common.PropertyService.FilePropertyProvider.FilePropertyProvider"/>
		<add key="propertyservice.providerassembly" value="td.common.propertyservice.filepropertyprovider"/>
		<add key="propertyservice.providers.fileprovider.filepath" value="D:\TransportDirect\Config\td.config.xml"/>
		<add key="themeinfrastructure.provider.type" value="file"/>
		<add key="themeinfrastructure.provider.theme.filepath" value="D:\TransportDirect\Config\Themes.xml"/>
		<add key="propertyservice.themeid" value="1"/>
	</appSettings>
	<system.runtime.remoting>
		<application>
			<channels>
				<channel ref="tcp" port="10001"/>
			</channels>
			<service>
				<wellknown mode="Singleton" 
					type="TransportDirect.JourneyPlanning.TTBO.TTBOInterface,td.TTBOInterface"
					objectUri="TTBOInterface"/>
			</service>
		</application>
	</system.runtime.remoting>
</configuration>
